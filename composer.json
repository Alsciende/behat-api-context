{
    "name": "alsciende/behat-api-context",
    "description": "Context to help test API with Behat",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alsciende",
            "email": "alsciende@icloud.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Alsciende\\Behat\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "beberlei/assert": "^2.9",
        "behat/behat": "^3.4",
        "symfony/browser-kit": "^4.1",
        "symfony/http-kernel": "^4.1",
        "symfony/property-access": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2"
    },
    "suggest": {
        "alsciende/behat-json-context": "Allows to test JSON responses",
        "alsciende/behat-doctrine-context": "Allows to test Doctrine data"
    },
    "config": {
        "sort-packages": true
    }
}
